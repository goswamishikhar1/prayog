{
  "version": 2,
  "name": "prayog-static",
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "home_page.html", "use": "@vercel/static" },
    { "src": "results.html", "use": "@vercel/static" },
    { "src": "detailed-results.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "results.css", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" },
    { "src": "airflow.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/$", "dest": "/index.html" },
    { "src": "/index(?:\\.html)?", "dest": "/index.html" },
    { "src": "/home", "dest": "/home_page.html" },
    { "src": "/home(?:\\.html)?", "dest": "/home_page.html" },
    { "src": "/results(?:\\.html)?", "dest": "/results.html" },
    { "src": "/detailed-results(?:\\.html)?", "dest": "/detailed-results.html" },
    { "src": "/(.*)", "status": 404, "dest": "/index.html" }
  ]
}


